    <script>
        $(document).ready(function () {
            getmachinelist();
        });
        $(function () {
            $('input[name="datetime"]').daterangepicker({
                showDropdowns: true,
                locale: {
                    format: 'MM/DD/YYYY'
                }
            });
            $('#txtcurrentreading').keyup(function () {
                if (!!$('#drpMachineNo').val()) {
                    var prR = $('#txtprevreading').val();
                    var fT = parseInt(this.value) - parseInt(prR);
                    $('#txttotalbalance').val(fT);
                }
                else {
                    this.value=null;
                    swal.fire('Information',"Please select Machine",'info');
                    return false;
                }

            });
        })
    </script>

